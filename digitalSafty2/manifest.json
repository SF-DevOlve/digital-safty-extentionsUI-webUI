{
    "manifest_version": 3,
    "name": "DigitalSafety2", 
    "version": "0.1",
    "description": "This extension will help you to stay safe online.",
    "permissions": ["tabs", "activeTab", "storage","scripting"],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
      ],
    "action": {
        "default_popup": "digitalSafety.html",
        "default_icon": {
            "16": "imgs/logo16.png",
            "32": "imgs/logo32.png",
            "128": "imgs/logo128.png"
        }
    },
      
    "icons": {
        "16": "imgs/logo16.png",
        "32": "imgs/logo32.png",
        "128": "imgs/logo128.png"
    }, 
    "background": {
        "service_worker": "background.js"
    }


    
}

   